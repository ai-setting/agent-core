{
  "name": "agent-core",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "tong_work": "./bin/tong_work"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun run scripts/build.ts",
    "build:ts": "tsc",
    "build:binary:single": "bun run scripts/build.ts --single",
    "build:release": "RELEASE=1 bun run scripts/build.ts",
    "dev": "bun run --conditions=browser ./src/cli/index.ts",
    "start": "bun run ./src/server/index.ts",
    "run": "bun run ./src/cli/index.ts run",
    "attach": "bun run ./src/cli/index.ts attach",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.37",
    "@ai-sdk/cerebras": "^2.0.30",
    "@ai-sdk/cohere": "^3.0.18",
    "@ai-sdk/deepinfra": "^2.0.31",
    "@ai-sdk/google": "^3.0.21",
    "@ai-sdk/groq": "^3.0.21",
    "@ai-sdk/mistral": "^3.0.18",
    "@ai-sdk/openai": "^3.0.25",
    "@ai-sdk/openai-compatible": "^2.0.27",
    "@ai-sdk/perplexity": "^3.0.17",
    "@ai-sdk/togetherai": "^2.0.30",
    "@ai-sdk/xai": "^3.0.47",
    "@types/eventsource": "^3.0.0",
    "@types/node": "^25.2.1",
    "ai": "^6.0.73",
    "eventsource": "^4.1.0",
    "glob": "^13.0.1",
    "hono": "^4.0.0",
    "solid-js": "^1.9.11",
    "yargs": "^18.0.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@types/bun": "^1.0.8",
    "@types/yargs": "^17.0.35",
    "typescript": "^5.3.3"
  }
}
